{
  "name": "linkorb/flex-auth-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provides flex auth for symfony in runtime",
  "keywords": ["auth"],
  "license": "MIT",
  "autoload":{
    "psr-4":{
      "FlexAuthBundle\\": "src/"
    }
  },
  "require": {
    "php": "^7.0",
    "symfony/config": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "linkorb/flex-auth": "master@dev"
  },
  "require-dev": {
  },
  "repositories": [
    {
      "type": "path",
      "url": "/var/www/flex-auth"
    }
  ]
}
